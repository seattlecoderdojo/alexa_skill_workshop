{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Federated": "www.amazon.com"
      },
      "Action": "sts:AssumeRoleWithWebIdentity",
      "Condition": {
        "StringEquals": {
          "www.amazon.com:user_id": "<%= @uid %>",
          "www.amazon.com:app_id": "amzn1.application-oa2-client.8ab3350dc66e454995bf763a50b67d04"
        }
      }
    }
  ]
}
